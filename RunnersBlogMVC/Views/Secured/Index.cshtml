@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Index</h1>

@using Microsoft.AspNetCore.Authentication

<h2>Claims</h2>

<d1>
    @foreach (var claim in User.Claims)
    {
        <dl>@claim.Type</dl>
        <dd>@claim.Value</dd> 
    }
</d1>
<h2>Properties</h2>

<d1>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dl>@prop.Key</dl>
        <dd>@prop.Value</dd>
    }
</d1>